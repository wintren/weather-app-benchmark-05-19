
geoAPI.geocode("London")
    .subscribeOn(Schedulers.io())
    .observeOn(AndroidSchedulers.mainThread())
    .subscribe({
        Log.d("TEMP", it)
    }, {
        it.printStackTrace()
    })


weatherAPI.getWeatherForecast(
    lat = 51.5073509,
    lon = -0.1277583
)
    .subscribeOn(Schedulers.io())
    .observeOn(AndroidSchedulers.mainThread())
    .subscribe({
        Log.d("TEMP", it)
    }, {
        it.printStackTrace()
    })
